{
    "request": {
        "method": "POST",
        "urlPath": "/cohere/v2/chat",
        "headers": {
            "Authorization": {
                "contains": "cohere-compression"
            }
        },
        "bodyPatterns": [
            {
                "contains": "files[5]{name,size,type}"
            }
        ]
    },
    "response": {
        "status": 200,
        "headers": {
            "Content-Type": "application/json"
        },
        "jsonBody": {
            "id": "chatcmpl-cohere-compression-enabled",
            "message": {
                "role": "assistant",
                "content": [
                    {
                        "type": "text",
                        "text": "I have processed the TOON-compressed tool result."
                    }
                ]
            },
            "finish_reason": "COMPLETE",
            "usage": {
                "billed_units": {
                    "input_tokens": 100,
                    "output_tokens": 10
                },
                "tokens": {
                    "input_tokens": 100,
                    "output_tokens": 10
                }
            }
        }
    }
}